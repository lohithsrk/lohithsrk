{
	"autorun": true,
	"autokill": true,
	"terminals": [
		{
			"name": "web",
			"cwd": "~/Downloads/portfolio-starter-main/Lohith-Portfolio",
			"commands": ["npm start"],

			"target": "My Other Terminal", 
			"split": "My Parent Terminal",
			"persistent": "unique_session_name",
			"substitution": false,
			"recycle": false,
			"open": true,
			"focus": true,
			"execute": false,
			"dynamicTitle": true,

			"onlySingle": true,
			"onlyMultiple": true,
			"onlyAPI": true,

			"shellPath": "/bin/bash",
			"shellArgs": ["--foo"],
			"env": { "name": "value" },
			"envInherit": false
		}
	]
}
